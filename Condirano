public class Condirano{
	public static void main(String[] args) {
		int[] id = new int[100];
		int i;
		int j;
		int k=1;
		for(i=0;i<id.length;i++){
			System.out.println(STR."第\{k++}行");
			for(j=i++;j<i;j++){
				System.out.println(STR."单数\{i}和双数\{j}");
				System.out.println("---------------------------");
			}
		}
		System.out.println(STR."总共\{id.length/2}条数据");
	}
}
